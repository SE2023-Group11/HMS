{"ast":null,"code":"import { defineComponent, reactive } from 'vue';\nexport default defineComponent({\n  name: 'DoctorDashboard',\n  setup() {\n    const state = reactive({\n      showAppointments: true,\n      appointments: [{\n        id: 1,\n        name: '张三',\n        date: '2023-05-01',\n        time: '上午',\n        isOver: false\n      }, {\n        id: 2,\n        name: '李四',\n        date: '2023-05-01',\n        time: '下午',\n        isOver: false\n      }, {\n        id: 3,\n        name: '王五',\n        date: '2023-05-02',\n        time: '上午',\n        isOver: false\n      }, {\n        id: 4,\n        name: '赵六',\n        date: '2023-05-02',\n        time: '下午',\n        isOver: false\n      }],\n      schedule: {\n        monday: {\n          morning: '出诊',\n          afternoon: '出诊',\n          evening: '休息'\n        },\n        tuesday: {\n          morning: '出诊',\n          afternoon: '休息',\n          evening: '出诊'\n        },\n        wednesday: {\n          morning: '休息',\n          afternoon: '出诊',\n          evening: '出诊'\n        },\n        thursday: {\n          morning: '出诊',\n          afternoon: '出诊',\n          evening: '休息'\n        },\n        friday: {\n          morning: '出诊',\n          afternoon: '休息',\n          evening: '出诊'\n        },\n        saturday: {\n          morning: '休息',\n          afternoon: '休息',\n          evening: '休息'\n        },\n        sunday: {\n          morning: '休息',\n          afternoon: '休息',\n          evening: '休息'\n        }\n      }\n    });\n    const markAsOver = appointment => {\n      appointment.isOver = true;\n    };\n    return {\n      ...state,\n      markAsOver\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","reactive","name","setup","state","showAppointments","appointments","id","date","time","isOver","schedule","monday","morning","afternoon","evening","tuesday","wednesday","thursday","friday","saturday","sunday","markAsOver","appointment"],"sources":["D:\\Desktop\\HMS\\HMS-front-end\\hms_frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>医生管理页面</h1>\r\n    <div>\r\n      <button @click=\"showAppointments = true\">预约列表</button>\r\n      <button @click=\"showAppointments = false\">出勤列表</button>\r\n    </div>\r\n    <div v-if=\"showAppointments\">\r\n      <h2>患者预约列表</h2>\r\n      <ul>\r\n        <li v-for=\"appointment in appointments\" :key=\"appointment.id\">\r\n          {{ appointment.name }} - {{ appointment.date }} - {{ appointment.time }}\r\n          <button @click=\"markAsOver(appointment)\">完成</button>\r\n          <span v-if=\"appointment.isOver\">over</span>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div v-else>\r\n      <h2>医生出勤列表</h2>\r\n      <ul>\r\n        <li>周一 上午：{{ schedule.monday.morning }}</li>\r\n        <li>周一 下午：{{ schedule.monday.afternoon }}</li>\r\n        <li>周一 晚上：{{ schedule.monday.evening }}</li>\r\n        <li>周二 上午：{{ schedule.tuesday.morning }}</li>\r\n        <li>周二 下午：{{ schedule.tuesday.afternoon }}</li>\r\n        <li>周二 晚上：{{ schedule.tuesday.evening }}</li>\r\n        <li>周三 上午：{{ schedule.wednesday.morning }}</li>\r\n        <li>周三 下午：{{ schedule.wednesday.afternoon }}</li>\r\n        <li>周三 晚上：{{ schedule.wednesday.evening }}</li>\r\n        <li>周四 上午：{{ schedule.thursday.morning }}</li>\r\n        <li>周四 下午：{{ schedule.thursday.afternoon }}</li>\r\n        <li>周四 晚上：{{ schedule.thursday.evening }}</li>\r\n        <li>周五 上午：{{ schedule.friday.morning }}</li>\r\n        <li>周五 下午：{{ schedule.friday.afternoon }}</li>\r\n        <li>周五 晚上：{{ schedule.friday.evening }}</li>\r\n        <li>周六 上午：{{ schedule.saturday.morning }}</li>\r\n        <li>周六 下午：{{ schedule.saturday.afternoon }}</li>\r\n        <li>周六 晚上：{{ schedule.saturday.evening }}</li>\r\n        <li>周日 上午：{{ schedule.sunday.morning }}</li>\r\n        <li>周日 下午：{{ schedule.sunday.afternoon }}</li>\r\n        <li>周日 晚上：{{ schedule.sunday.evening }}</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, reactive } from 'vue';\r\n\r\nexport default defineComponent({\r\n  name: 'DoctorDashboard',\r\n  setup() {\r\n    const state = reactive({\r\n      showAppointments: true,\r\n      appointments: [\r\n        { id: 1, name: '张三', date: '2023-05-01', time: '上午', isOver: false },\r\n        { id: 2, name: '李四', date: '2023-05-01', time: '下午', isOver: false },\r\n        { id: 3, name: '王五', date: '2023-05-02', time: '上午', isOver: false },\r\n        { id: 4, name: '赵六', date: '2023-05-02', time: '下午', isOver: false },\r\n      ],\r\n      schedule: {\r\n        monday: { morning: '出诊', afternoon: '出诊', evening: '休息' },\r\n        tuesday: { morning: '出诊', afternoon: '休息', evening: '出诊' },\r\n        wednesday: { morning: '休息', afternoon: '出诊', evening: '出诊' },\r\n        thursday: { morning: '出诊', afternoon: '出诊', evening: '休息' },\r\n        friday: { morning: '出诊', afternoon: '休息', evening: '出诊' },\r\n        saturday: { morning: '休息', afternoon: '休息', evening: '休息' },\r\n        sunday: { morning: '休息', afternoon: '休息', evening: '休息' },\r\n      },\r\n    });\r\n\r\n    const markAsOver = (appointment) => {\r\n      appointment.isOver = true;\r\n    };\r\n\r\n    return {\r\n      ...state,\r\n      markAsOver,\r\n    };\r\n  },\r\n});\r\n</script>"],"mappings":"AA+CA,SAASA,eAAe,EAAEC,QAAO,QAAS,KAAK;AAE/C,eAAeD,eAAe,CAAC;EAC7BE,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACrBI,gBAAgB,EAAE,IAAI;MACtBC,YAAY,EAAE,CACZ;QAAEC,EAAE,EAAE,CAAC;QAAEL,IAAI,EAAE,IAAI;QAAEM,IAAI,EAAE,YAAY;QAAEC,IAAI,EAAE,IAAI;QAAEC,MAAM,EAAE;MAAM,CAAC,EACpE;QAAEH,EAAE,EAAE,CAAC;QAAEL,IAAI,EAAE,IAAI;QAAEM,IAAI,EAAE,YAAY;QAAEC,IAAI,EAAE,IAAI;QAAEC,MAAM,EAAE;MAAM,CAAC,EACpE;QAAEH,EAAE,EAAE,CAAC;QAAEL,IAAI,EAAE,IAAI;QAAEM,IAAI,EAAE,YAAY;QAAEC,IAAI,EAAE,IAAI;QAAEC,MAAM,EAAE;MAAM,CAAC,EACpE;QAAEH,EAAE,EAAE,CAAC;QAAEL,IAAI,EAAE,IAAI;QAAEM,IAAI,EAAE,YAAY;QAAEC,IAAI,EAAE,IAAI;QAAEC,MAAM,EAAE;MAAM,CAAC,CACrE;MACDC,QAAQ,EAAE;