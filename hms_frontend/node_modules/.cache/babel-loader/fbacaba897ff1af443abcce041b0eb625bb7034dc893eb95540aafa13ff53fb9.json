{"ast":null,"code":"import axios from 'axios';\nimport { defineComponent, ref } from 'vue';\nimport { Button } from 'primevue/button';\nimport { Card } from 'primevue/card';\nimport { Listbox } from 'primevue/listbox';\nimport { useRoute, useRouter } from 'vue-router';\nimport Vue from 'vue';\nexport default {\n  name: 'App',\n  directives: {\n    fixedFooter: {\n      mounted(el) {\n        // 初始化页面时，先调整一次页脚位置\n        positionFooter(el);\n        // 监听窗口大小变化和滚动事件\n        window.addEventListener('resize', () => positionFooter(el));\n        window.addEventListener('scroll', () => positionFooter(el));\n      }\n    }\n  },\n  computed: {\n    footerPosition() {\n      const wrapper = document.querySelector('.page-wrapper');\n      const footer = document.querySelector('.page-footer');\n\n      // 获取窗口高度、页面内容高度和滚动条高度\n      const windowHeight = window.innerHeight;\n      const contentHeight = wrapper.offsetHeight;\n      const scrollHeight = wrapper.scrollHeight;\n      const scrollTop = window.pageYOffset || document.documentElement.scrollTop;\n\n      // 计算页脚元素的位置\n      let position = 'static';\n      if (scrollHeight > windowHeight) {\n        if (scrollTop + windowHeight >= scrollHeight) {\n          position = 'absolute';\n        } else {\n          position = 'relative';\n        }\n      } else {\n        if (contentHeight + footer.offsetHeight < windowHeight) {\n          position = 'absolute';\n        } else {\n          position = 'relative';\n        }\n      }\n      return position;\n    }\n  },\n  mounted() {\n    // 初始化页面时，先调整一次页脚位置\n    this.$nextTick(() => {\n      const footer = this.$refs.footer;\n      positionFooter(footer);\n      window.addEventListener('resize', () => positionFooter(footer));\n      window.addEventListener('scroll', () => positionFooter(footer));\n    });\n  },\n  setup() {\n    const showMenu = true;\n    const route = useRoute();\n    const router = useRouter();\n    return {\n      route,\n      router\n    };\n  },\n  components: {\n    Button,\n    Card,\n    Listbox\n  }\n};\nfunction positionFooter(el) {\n  el.style.position = el.closest('.page-wrapper').querySelector('.page-footer').getAttribute('data-fixed-footer');\n}","map":{"version":3,"names":["axios","defineComponent","ref","Button","Card","Listbox","useRoute","useRouter","Vue","name","directives","fixedFooter","mounted","el","positionFooter","window","addEventListener","computed","footerPosition","wrapper","document","querySelector","footer","windowHeight","innerHeight","contentHeight","offsetHeight","scrollHeight","scrollTop","pageYOffset","documentElement","position","$nextTick","$refs","setup","showMenu","route","router","components","style","closest","getAttribute"],"sources":["D:\\Desktop\\HMS-front-end\\hms_frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-wrapper\">\r\n    <div class=\"header\">\r\n      <div class=\"logo\">\r\n        <i class=\"pi pi-hospital\"></i>\r\n        <img class=\"img_top\" src=\"./Pic/img.jpeg\" alt=\"User Avatar\"/>\r\n        <span display=\"flex\" >医院信息管理系统</span>\r\n      </div>\r\n      <div class=\"user-info\" @mouseover=\"showMenu = true\" @mouseleave=\"showMenu = false\">\r\n        <img class=\"avatar\" src=\"https://f.pz.al/pzal/2023/05/03/5e6420e7ffe6f.png\" alt=\"User Avatar\"/>\r\n        <div v-if=\"showMenu\" class=\"user-menu\">\r\n          <ul>\r\n            <li> <router-link to=\"/message\">消息通知</router-link></li>\r\n            <li><a href=\"/login\">退出登录</a></li>\r\n            <li><a href=\"/space\">个人信息</a></li>\r\n          </ul>\r\n        </div>\r\n      </div> \r\n    </div>\r\n    <div class=\"content\">\r\n <router-view></router-view>\r\n    </div>\r\n  <div class=\"page-footer\">\r\n      <div class=\"left\">\r\n      <p>版权所有 2014-2022 北京航空航天大学<br>\r\n         京ICP备05004617-3<br>\r\n         文保网安备案号1101080018</p>\r\n      </div>\r\n      <img class=\"img_bottom\" src=\"./Pic/img.jpeg\" />\r\n    <div class=\"right\">\r\n    <p>地址：北京市海淀区学院路37号<br> \r\n       邮编：100191<br>\r\n       电话：82317114</p>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script> \r\nimport axios from 'axios';\r\nimport { defineComponent, ref } from 'vue';\r\nimport { Button } from 'primevue/button';\r\nimport { Card } from 'primevue/card';\r\nimport { Listbox } from 'primevue/listbox';\r\nimport { useRoute, useRouter } from 'vue-router';\r\nimport Vue from 'vue';\r\nexport default {\r\n  name:'App',\r\n  directives: {\r\n    fixedFooter: {\r\n      mounted(el) {\r\n        // 初始化页面时，先调整一次页脚位置\r\n        positionFooter(el);\r\n        // 监听窗口大小变化和滚动事件\r\n        window.addEventListener('resize', () => positionFooter(el));\r\n        window.addEventListener('scroll', () => positionFooter(el));\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    footerPosition() {\r\n      const wrapper = document.querySelector('.page-wrapper');\r\n      const footer = document.querySelector('.page-footer');\r\n\r\n      // 获取窗口高度、页面内容高度和滚动条高度\r\n      const windowHeight = window.innerHeight;\r\n      const contentHeight = wrapper.offsetHeight;\r\n      const scrollHeight = wrapper.scrollHeight;\r\n      const scrollTop = window.pageYOffset || document.documentElement.scrollTop;\r\n\r\n      // 计算页脚元素的位置\r\n      let position = 'static';\r\n      if (scrollHeight > windowHeight) {\r\n        if (scrollTop + windowHeight >= scrollHeight) {\r\n          position = 'absolute';\r\n        } else {\r\n          position = 'relative';\r\n        }\r\n      } else {\r\n        if (contentHeight + footer.offsetHeight < windowHeight) {\r\n          position = 'absolute';\r\n        } else {\r\n          position = 'relative';\r\n        }\r\n      }\r\n      return position;\r\n    }\r\n  },\r\n  mounted() {\r\n    // 初始化页面时，先调整一次页脚位置\r\n    this.$nextTick(() => {\r\n    const footer = this.$refs.footer;\r\n    positionFooter(footer);\r\n    window.addEventListener('resize', () => positionFooter(footer));\r\n    window.addEventListener('scroll', () => positionFooter(footer));\r\n  });\r\n  },\r\n  setup() {\r\n    const showMenu=true;\r\n      const route = useRoute();\r\n    const router = useRouter();\r\n      return {route, router};},\r\n  components: {\r\n    Button,\r\n    Card,\r\n    Listbox,\r\n  }\r\n}\r\nfunction positionFooter(el) {\r\n  el.style.position = el.closest('.page-wrapper').querySelector('.page-footer').getAttribute('data-fixed-footer');\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n  .img_top{\r\n    height: 80px;\r\n  }\r\n  .header {\r\n      position: absolute; /* 绝对定位 */\r\n      top: 0; /* 从顶部开始 */\r\n      left: 0; /* 从左边开始 */\r\n      width: 100%; /* 宽度占满整个页面 */\r\n      height: 80px; /* 设置高度 */\r\n      background-color: #ffffff; /* 设置背景颜色 */\r\n  }\r\n  .avatar{\r\n    position: absolute;\r\n    top:10px;\r\n    right: 50px;\r\n  }\r\n  .rowTitle{\r\n    width: 100px;\r\n    font-size: 10px;\r\n  }\r\n  .logo {\r\n    display: flex;\r\n    align-items: center;\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .logo i {\r\n    font-size: 32px;\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .user-info {\r\n    align-items: center;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .user-info img {\r\n    width: 60px;\r\n    height: 60px;\r\n    border-radius: 50%;\r\n    margin-left: 60px;\r\n  }\r\n  \r\n  .user-menu {\r\n    position: absolute;\r\n    top:70px;\r\n    right: 0;\r\n    width: 160px;\r\n    background-color: #fff;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n    box-shadow: 0 2px 4px rgba(255, 255, 255, 0.2);\r\n  }\r\n  \r\n  .user-menu ul {\r\n    list-style: none;\r\n    padding: 0;\r\n    margin: 0;\r\n  }\r\n  \r\n  .user-menu li {\r\n    border-bottom: 1px solid #ccc;\r\n  }\r\n  \r\n  .user-menu li:last-child {\r\n    border-bottom: none;\r\n  }\r\n  \r\n  .user-menu a {\r\n    display: block;\r\n    padding: 10px;\r\n    color: #333;\r\n  }\r\n  .content {\r\n    min-height: calc(100vh - 100px); /* 计算内容区域的最小高度 */\r\n  }\r\n  .user-menu a:hover {\r\n    background-color:rgb(255, 255, 255);\r\n  }\r\n  .page-footer {\r\n    position: absolute; /* 绝对定位 */\r\n    bottom: 0; /* 从底部开始 */\r\n    left: 0; /* 从左边开始 */\r\n    width: 100%; /* 宽度占满整个页面 */\r\n    height: 50px; /* 设置高度 */\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-top: 20px;\r\n    font-size: 1.2px;\r\n    background-color:rgb(255, 255, 255);\r\n  }\r\n  .page-footer .left p {\r\n    bottom: 0;\r\n    left: 50px;\r\n    width: 300px;\r\n    height: 50px;\r\n    overflow: auto;\r\n  }\r\n  .img_bottom {\r\n    height: 100px;\r\n    left:550px;\r\n  }\r\n  .page-footer .right p {\r\n    bottom: 0;\r\n    right: 50px;\r\n    width: 300px;\r\n    height: 50px;\r\n    overflow: auto;\r\n  }\r\n  </style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,eAAe,EAAEC,GAAE,QAAS,KAAK;AAC1C,SAASC,MAAK,QAAS,iBAAiB;AACxC,SAASC,IAAG,QAAS,eAAe;AACpC,SAASC,OAAM,QAAS,kBAAkB;AAC1C,SAASC,QAAQ,EAAEC,SAAQ,QAAS,YAAY;AAChD,OAAOC,GAAE,MAAO,KAAK;AACrB,eAAe;EACbC,IAAI,EAAC,KAAK;EACVC,UAAU,EAAE;IACVC,WAAW,EAAE;MACXC,OAAOA,CAACC,EAAE,EAAE;QACV;QACAC,cAAc,CAACD,EAAE,CAAC;QAClB;QACAE,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,MAAMF,cAAc,CAACD,EAAE,CAAC,CAAC;QAC3DE,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,MAAMF,cAAc,CAACD,EAAE,CAAC,CAAC;MAC7D;IACF;EACF,CAAC;EACDI,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,MAAMC,OAAM,GAAIC,QAAQ,CAACC,aAAa,CAAC,eAAe,CAAC;MACvD,MAAMC,MAAK,GAAIF,QAAQ,CAACC,aAAa,CAAC,cAAc,CAAC;;MAErD;MACA,MAAME,YAAW,GAAIR,MAAM,CAACS,WAAW;MACvC,MAAMC,aAAY,GAAIN,OAAO,CAACO,YAAY;MAC1C,MAAMC,YAAW,GAAIR,OAAO,CAACQ,YAAY;MACzC,MAAMC,SAAQ,GAAIb,MAAM,CAACc,WAAU,IAAKT,QAAQ,CAACU,eAAe,CAACF,SAAS;;MAE1E;MACA,IAAIG,QAAO,GAAI,QAAQ;MACvB,IAAIJ,YAAW,GAAIJ,YAAY,EAAE;QAC/B,IAAIK,SAAQ,GAAIL,YAAW,IAAKI,YAAY,EAAE;UAC5CI,QAAO,GAAI,UAAU;QACvB,OAAO;UACLA,QAAO,GAAI,UAAU;QACvB;MACF,OAAO;QACL,IAAIN,aAAY,GAAIH,MAAM,CAACI,YAAW,GAAIH,YAAY,EAAE;UACtDQ,QAAO,GAAI,UAAU;QACvB,OAAO;UACLA,QAAO,GAAI,UAAU;QACvB;MACF;MACA,OAAOA,QAAQ;IACjB