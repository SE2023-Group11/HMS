ios.delete(`http://121.199.161.134:8080/deleteAppointment`, params.toString()).then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    cancelAppointment() {\n      alert(\"取消预约\");\n      this.$router.go(-1); // 返回上一页\n    },\n\n    indexToTime(index) {\n      if (index >= 0 && index <= 6) {\n        return this.morningTimes[index];\n      } else {\n        return this.afternoonTimes[index - 7];\n      }\n    }\n  }\n};","map":{"version":3,"names":["DemoBottom","DemoTitle","axios","name","components","data","morningTimes","afternoonTimes","doctorId","$route","query","doctorName","doctorTitle","date","time","sectionFirName","sectionSecName","methods","confirmAppointment","params","URLSearchParams","append","console","log","toString","post","then","response","alert","$router","go","catch","error","deleteAppointment","orderId","delete","cancelAppointment","indexToTime","index"],"sources":["D:\\Desktop\\HMS-front-end\\hms_frontend\\src\\views\\ConfirmAppointment.vue"],"sourcesContent":["<template>\r\n  <DemoTitle/>\r\n\r\n  <div class=\"appointment-info\">\r\n    <p>PatientID: {{ patientId }}</p>\r\n    <p>DoctorID: {{ doctorId }}</p>\r\n    <p>Date: {{ date }}</p>\r\n    <p>Time: {{ indexToTime(time) }}</p>\r\n    <p>doctorName:{{ doctorName }}</p>\r\n    <p>doctorTitle:{{ doctorTitle }}</p>\r\n    <p>sectionFirName:{{ sectionFirName }}</p>\r\n    <p>sectionSecName:{{ sectionSecName }}</p>\r\n  </div>\r\n  <div class=\"buttons\">\r\n    <button @click=\"confirmAppointment\">确定预约</button>\r\n    <button @click=\"cancelAppointment\">取消预约</button>\r\n  </div>\r\n\r\n  <DemoBottom/>\r\n</template>\r\n\r\n<script>\r\nimport DemoBottom from '../components/DemoBottom.vue';\r\nimport DemoTitle from '../components/DemoTitle.vue';\r\nimport axios from 'axios';\r\nexport default {\r\n  name:\"ConfirmAppointment\",\r\n  components:{\r\n    DemoBottom,\r\n    DemoTitle\r\n  },\r\n  data(){\r\n    return{\r\n      morningTimes: [\r\n        '8:00-8:30',\r\n        '8:30-9:00',\r\n        '9:00-9:30',\r\n        '9:30-10:00',\r\n        '10:00-10:30',\r\n        '10:30-11:00',\r\n        '11:00-11:30',\r\n      ],\r\n      afternoonTimes: [\r\n        '14:00-14:30',\r\n        '14:30-15:00',\r\n        '15:00-15:30',\r\n        '15:30-16:00',\r\n        '16:00-16:30',\r\n        '16:30-17:00',\r\n      ],\r\n      doctorId: this.$route.query.doctorId,\r\n      doctorName: this.$route.query.doctorName,\r\n      doctorTitle: this.$route.query.doctorTitle,\r\n      date: this.$route.query.date,\r\n      time: this.$route.query.time,\r\n      sectionFirName:\"zhangsan\",\r\n      sectionSecName:\"lisi\",\r\n      //sectionFirName: this.$route.query.sectionFirName,\r\n      //sectionSecName: this.$route.query.sectionSecName,\r\n    }\r\n  },\r\n  methods: {\r\n    confirmAppointment() {\r\n      const params = new URLSearchParams();\r\n      params.append('token', 'eyJhbGciOiJIUzI1NiJ9.eyJub3dMb2dnZWRJblR5cGUiOiJub3dMb2dnZWRJblR5cGVQYXRpZW50Iiwibm93TG9nZ2VkSW5JZCI6IlAwMDAwMDAwMDAwMCIsImlhdCI6MTY4NDc0NTUxOCwiZXhwIjoxNjg2NTQ1NTE4fQ.5dh7XJTkDsaVpHrsTBw4YGs8lnKdY1GRnNCgbJZLtC0');\r\n      params.append('doctorId', this.doctorId);\r\n      params.append('day', this.date);\r\n      params.append('time', this.time);\r\n      console.log(this.doctorId);\r\n      console.log(params.toString());\r\n      axios.post('http://121.199.161.134:8080/addAppointment', params.toString())\r\n        .then(response => {\r\n          console.log(response.data);\r\n          alert(\"成功预约，请在‘我的预约’中查询\");\r\n          this.$router.go(-1); // 返回上一页\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteAppointment(orderId) {\r\n      alert(\"delete\");\r\n      const params = new URLSearchParams();\r\n      params.append('token','5BCDA9DC10ED23F86EC5F3C6FFC70BFE');\r\n      params.append('orderid',orderId);\r\n      axios.delete(`http://121.199.161.134:8080/deleteAppointment`,params.toString())\r\n      .then(response => {\r\n        console.log(response);\r\n      })\r\n      .catch(error => {\r\n        console.error(error);\r\n      })\r\n    },\r\n\r\n\r\n    cancelAppointment() {\r\n      alert(\"取消预约\");\r\n      this.$router.go(-1); // 返回上一页\r\n    },\r\n    indexToTime(index){\r\n      if(index >= 0 && index <= 6){\r\n        return this.morningTimes[index]; \r\n      }else{\r\n        return this.afternoonTimes[index-7];\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.appointment-info {\r\n  margin-top: 20px;\r\n  font-size: 16px;\r\n  line-height: 1.5;\r\n}\r\n.buttons {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\nbutton {\r\n  padding: 10px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\nbutton:hover {\r\n  background-color: #0062cc;\r\n}\r\n</style>"],"mappings":"AAsBA,OAAOA,UAAS,MAAO,8BAA8B;AACrD,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAC,oBAAoB;EACzBC,UAAU,EAAC;IACTJ,UAAU;IACVC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,YAAY,EAAE,CACZ,WAAW,EACX,WAAW,EACX,WAAW,EACX,YAAY,EACZ,aAAa,EACb,aAAa,EACb,aAAa,CACd;MACDC,cAAc,EAAE,CACd,aAAa,EACb,aAAa,EACb,aAAa,EACb,aAAa,EACb,aAAa,EACb,aAAa,CACd;MACDC,QAAQ,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,QAAQ;MACpCG,UAAU,EAAE,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,UAAU;MACxCC,WAAW,EAAE,IAAI,CAACH,MAAM,CAACC,KAAK,CAACE,WAAW;MAC1CC,IAAI,EAAE,IAAI,CAACJ,MAAM,CAACC,KAAK,CAACG,IAAI;MAC5BC,IAAI,EAAE,IAAI,CAACL,MAAM,CAACC,KAAK,CAACI,IAAI;MAC5BC,cAAc,EAAC,UAAU;MACzBC,cAAc,EAAC;MACf;MACA;IACF;EACF,CAAC;;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,MAAMC,MAAK,GAAI,IAAIC,eAAe,EAAE;MACpCD,MAAM,CAACE,MAAM,CAAC,OAAO,EAAE,qNAAqN,CAAC;MAC7OF,MAAM,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACb,QAAQ,CAAC;MACxCW,MAAM,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAACR,IAAI,CAAC;MAC/BM,MAAM,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACP,IAAI,CA