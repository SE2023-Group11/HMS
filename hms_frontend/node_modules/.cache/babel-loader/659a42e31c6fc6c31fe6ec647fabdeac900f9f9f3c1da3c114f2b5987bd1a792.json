 axios.delete(`http://121.199.161.134:8080/deleteAppointment`, params.toString()).then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    cancelAppointment() {\n      alert(\"取消预约\");\n      this.$router.go(-1); // 返回上一页\n    },\n\n    indexToTime(index) {\n      if (index >= 0 && index <= 6) {\n        return this.morningTimes[index];\n      } else {\n        return this.afternoonTimes[index - 7];\n      }\n    },\n    showList() {\n      var list = this.$refs.headerList;\n      list.style.display = \"block\";\n    },\n    unShowList() {\n      var list = this.$refs.headerList;\n      list.style.display = \"none\";\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","morningTimes","afternoonTimes","doctorId","$route","query","doctorName","doctorTitle","date","time","methods","goToPatientSpace","$router","push","goToDelete","goToLogin","goToMessage","confirmAppointment","params","URLSearchParams","append","post","toString","then","response","console","log","code","alert","go","catch","error","deleteAppointment","orderId","delete","cancelAppointment","indexToTime","index","showList","list","$refs","headerList","style","display","unShowList"],"sources":["D:\\Desktop\\HMS-front-end\\hms_frontend\\src\\views\\ConfirmAppointment.vue"],"sourcesContent":["<template>\r\n  <div class=\"background\"></div>\r\n  <div class=\"container\" id=\"container\">\r\n    <div class=\"header\">\r\n      <img src=\"https://f.pz.al/pzal/2023/05/19/d218206d1e4dd.png\" alt=\"\" class=\"header_img\" />\r\n      <h1 class=\"header_tag\">HMS医院门诊预约系统</h1>\r\n      <div class=\"header_user\" @mouseenter=\"showList\" @mouseleave=\"unShowList\">\r\n        <!-- 显示头像 -->\r\n        <img src=\"https://f.pz.al/pzal/2023/05/03/5e6420e7ffe6f.png\" alt=\"\" class=\"header_user_img\" />\r\n        <!-- 未登录时显示登录/注册 -->\r\n        <!-- 登录后显示用户名 -->\r\n        <h1 class=\"header_user_word\">登录/注册</h1>\r\n        <div 